algoritmo "semnome"

var
 massa, massai: real
 tempo: inteiro
inicio
 escreva("Informe a massa em gramas: ")
 leia(massa)
 massai<-massa
 
 enquanto (massa>=0.5) faca
 massa<-massa/2
 tempo<-tempo+60
 fimenquanto
 

 escreval("A nassa inicial e: ",massai)
 escreval("A massa final e: ",massa)
 escreval("O tempo gasto foi: ",tempo)
 

 
 

fimalgoritmo
