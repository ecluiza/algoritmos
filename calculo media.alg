algoritmo "calcular"

var
   nota1, nota2, nota3, nota4, mediaf: real
inicio
   escreva("informe a nota1: ")
   leia(nota1)
   escreva("informe a nota2: ")
   leia(nota2)
   escreva("informe a nota3: ")
   leia(nota3)
   escreva("informe a nota4: ")
   leia(nota4)

   mediaf <- (nota1+ nota2+ nota3+ nota4)/4
   
   escreva("a media final é: ", mediaf)
   
fimalgoritmo
