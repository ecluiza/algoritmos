algoritmo "altura 20 pessoas"

var
  contador, altura, media, soma, maior: inteiro
inicio
   soma<-0
   para contador <- 1 ate 20 faca
   escreva("Informe a altura: ")
   leia(altura)
   
   soma<- soma+altura
   media<-soma/20
   
   se(altura>2)entao
   maior<-altura
   fimse
   

   

fimalgoritmo
